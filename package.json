{
  "name": "boilerplate-vue-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "license": "AGPLv3.0",
  "scripts": {
    "dev": "vite --port 8080",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "nightwatch tests/e2e/*",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@guebbit/css-toolkit": "^1.0.1",
    "@guebbit/css-ui": "^1.0.1",
    "@guebbit/javascript-library": "^1.0.5",
    "axios": "^1.7.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.27",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nightwatch/vue": "^3.1.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/nightwatch": "^2.3.30",
    "@types/node": "^22.4.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.27.0",
    "geckodriver": "^4.2.1",
    "jsdom": "^24.0.0",
    "nightwatch": "^3.6.3",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-nightwatch": "^0.4.5",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.19"
  }
}
