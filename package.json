{
  "name": "boilerplate-vue-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "AGPLv3.0",
  "scripts": {
    "dev": "vite --port 8080",
    "build": "run-p type-check-only \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check-only": "vue-tsc --build --force",
    "lint": "eslint",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:target": "vitest run src/components/atoms/__tests__/CounterInput.spec.ts",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:target": "start-server-and-test preview http://localhost:4173 'cypress run --e2e' --spec 'tests/e2e/xxxx.spec.ts'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "node:kill": "cmd \"/C TASKKILL /IM node.exe /F\"",
    "format": "prettier --write src/",
    "prepublish": "npm run lint && npm run test:unit && npm run build && npm run docs:build"
  },
  "dependencies": {
    "@guebbit/css-toolkit": "^1.1.3",
    "@guebbit/css-ui": "file:../css-ui",
    "@guebbit/js-toolkit": "^1.0.3",
    "axios": "^1.7.2",
    "minisearch": "^7.1.0",
    "pinia": "^2.2.4",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/cypress": "^1.1.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.11.0",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.79.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-nightwatch": "^0.4.6",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vitepress": "^1.4.0",
    "vitest": "^2.1.2",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6"
  }
}
