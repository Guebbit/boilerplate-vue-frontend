{
  "name": "boilerplate-vue-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "AGPLv3.0",
  "scripts": {
    "dev": "vite --port 8080",
    "build": "run-p type-check-only \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check-only": "vue-tsc --build --force",
    "lint": "eslint",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:target": "vitest run src/components/atoms/__tests__/CounterInput.spec.ts",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:target": "start-server-and-test preview http://localhost:4173 'cypress run --e2e' --spec 'tests/e2e/xxxx.spec.ts'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "kill:win": "cmd \"/C TASKKILL /IM node.exe /F\"",
    "kill:gnu": "pkill -9 node",
    "format": "prettier --write src/",
    "prepublish": "npm run lint && npm run test:unit && npm run build && npm run docs:build"
  },
  "dependencies": {
    "@guebbit/css-toolkit": "1.3.2",
    "@guebbit/js-toolkit": "^1.0.3",
    "axios": "^1.8.1",
    "minisearch": "^7.1.2",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.1",
    "vue-router": "^4.5.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.7",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.2",
    "sass-embedded": "^1.85.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitepress": "^1.6.3",
    "vitest": "^3.0.7",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.8"
  }
}
